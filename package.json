{
  "name": "kaggle-to-notebook",
  "displayName": "Kaggle to Notebook",
  "description": "Extracts Python code from a Kaggle notebook link",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kaggle-to-notebook.fetchKaggleNotebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kaggle-to-notebook.fetchKaggleNotebook",
        "title": "Fetch Kaggle Notebook"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "cheerio": "^1.0.0",
    "kaggle-to-notebook": "file:"
  }
}